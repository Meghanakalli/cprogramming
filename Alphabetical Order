#include<stdio.h>
#include<string.h>
int main()
{
	int i,j,n;
	char str[10][10],s[10];
	printf("enter number of names=\n");
	scanf("%d",&n);
	printf("enter names in descending order=\n");
	for(i=0;i<n;i++)
	{
		scanf("%s",&str[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if(strcmp(str[i],str[j])>0)
			{
				strcpy(s,str[i]);
				strcpy(str[i],str[j]);
				strcpy(str[j],s);
			}
		}
	}
	printf("\nthe sorted orderof names are=\n");
	for(i=0;i<n;i++)
	{
		printf("%s\n",str[i]);
	}
	return 0;
}
#include<stdio.h>
#include<string.h>
int main()
{
	int i,j,n;
	char str[10][10],s[10];
	printf("enter number of names=\n");
	scanf("%d",&n);
	printf("enter names in descending order=\n");
	for(i=0;i<n;i++)
	{
		scanf("%s",&str[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if(strcmp(str[i],str[j])>0)
			{
				strcpy(s,str[i]);
				strcpy(str[i],str[j]);
				strcpy(str[j],s);
			}
		}
	}
	printf("\nthe sorted orderof names are=\n");
	for(i=0;i<n;i++)
	{
		printf("%s\n",str[i]);
	}
	return 0;
}
